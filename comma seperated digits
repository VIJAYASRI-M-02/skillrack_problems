input : 4
        56e43d247f432
output:
      56,43,247,432


#include <stdio.h>
#include<stdlib.h>
#include<string.h>
#include <ctype.h>

int main()
{
    int n;
    scanf("%d",&n);
    char s[1000];
    scanf("%s",s);
    for(int i=0;i<strlen(s);i++){
        if(!(isalpha(s[i]))){
            printf("%c",s[i]);
        }
        else{
            printf(",");
        }
    }

    return 0;
}
